


<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Online Bookstore</title>
    <link rel="stylesheet" href="styles.css">
</head>

<body>
    <h1>Online Bookstore</h1>
    <div class="navbar">
        <button class="menu-button">Menu</button>
        <div class="search-container">
            <input type="text" placeholder="Search...">
            <button class="search-button">Search</button>
        </div>
    </div>
    <div id="bookstore">
        <h2>Books Available</h2>
        <div id="books" class="grid-container">
            <!-- Book list will be dynamically added here -->
        </div>
        <h2>Shopping Cart</h2>
        <div id="cart" class="grid-container">
            <!-- Shopping cart will be dynamically updated here -->

            <div class="total-cost">
                <p>Total Cost: $<span id="totalCost">0</span></p>
                <button class="order-button">Order</button>
            </div>
        </div>
    </div>

    <script>
        // Sample book data
        const books = [
            { title: "aliens", author: "manishi", price: 10.99, image: "https://www.blackgate.com/wp-content/uploads/2018/04/The-Collectors-Book-of-Science-Fiction.jpg" },
            { title: "sharon", author: "gurthu thelidu", price: 15.99, image: "https://www.giantfreakinrobot.com/wp-content/uploads/2019/11/agent.jpg" },
            { title: "science letrcher", author: "yavado okadu", price: 12.49, image: "https://i.pinimg.com/originals/dd/a5/ba/dda5ba79b078f466b5b38ea750eabe38.jpg" },
            { title: "gun", author: "gun the0.", price: 9.99, image: "https://i.pinimg.com/originals/49/ed/e5/49ede50560681c25baf9dac5c5a69ed9.jpg" },
            // Add more books with images here
            { title: "Your Book Title 1", author: "Author 1", price: 12.99, image: "https://imaginationsoup.net/wp-content/uploads/2020/05/Finn-and-the-Intergalactic-Lunchbox-1356x2048.jpg" },
            { title: "Your Book Title 2", author: "Author 2", price: 9.49, image: "https://www.blackgate.com/wp-content/uploads/2020/11/Escape-Pod-The-Science-Fiction-Anthology.jpg" },
            { title: " avatar", author: "james camaron", price: 10.49, image: "https://lumiere-a.akamaihd.net/v1/images/o_avatar_comicbookcovers_4_22718_34a2e26e.jpeg?region=0,0,480,738" },
            { title: "inseption", author: "nolan maya", price: 10.00, image: "https://m.media-amazon.com/images/I/91cVXf75GXL._AC_UF1000,1000_QL80_.jpg" },
            { title: "Ben 10", author: "Max", price: 15, image: "https://www.mbibooks.com/mbibooks_details/uploads/2021/05/BEN-10-ALIEN-FORCE1.jpg" },
            { title: "dragob ballz", author: "yakomo", price: 21, image: "https://m.media-amazon.com/images/I/51ndONkZ4gL._AC_UF1000,1000_QL80_.jpg " },

            { title : "Monsters", author : "Barry Windsor-Smith", price : 39.99, image : "https://th.bing.com/th/id/OIP.HcFBf350qalBiWCnOPkc8QHaHa?pid=ImgDet&rs=1" },
            { title : "The Good Asian", author : "Pornsak Pichetshote and Alexandre Tefenkgi", price : 9.99, image : "https://th.bing.com/th/id/OIP.l9MI04C1xDkGbydfgPKzGAHaLZ?w=201&h=310&c=7&r=0&o=5&dpr=1.3&pid=1.7" },
            { title : "Daredevil", author : "Chip Zdarsky and Marco Checchetto", price : 3.99, image : "https://th.bing.com/th/id/OIP.Cd4-qpqROsAViADrNjbDkAHaLU?w=202&h=309&c=7&r=0&o=5&dpr=1.3&pid=1.7" },
            { title : "After the Rain", author : "John Jennings, Nnedi Okorafor and David Brame", price : 24.99, image : "https://th.bing.com/th/id/OIP.sAzQcBW9rb46Iw3cfTfuHwHaLi?w=200&h=311&c=7&r=0&o=5&dpr=1.3&pid=1.7" },
            { title : "Saga", author : "Brian K. Vaughan and Fiona Staples", price : 9.99, image : "data:image/jpeg;base64," },
            { title : "Watchmen", author : "Alan Moore and Dave Gibbons", price : 24.99, image : "https://th.bing.com/th/id/OIP.uuM8fHOm2PtnOQtVvcbVmgHaLV?w=202&h=309&c=7&r=0&o=5&dpr=1.3&pid=1.7  " },
            { title : "Maus", author : "Art Spiegelman", price : 15.95 , image : "https://th.bing.com/th/id/OIP.nrFquQdCLCYhrcgE1FAZSQHaJ4?w=129&h=180&c=7&r=0&o=5&dpr=1.3&pid=1.7" },
            { title : "Persepolis", author : "Marjane Satrapi", price : 14.95, image : "https://th.bing.com/th/id/OIP.xCmwGoFhz7XuvZeA53uRigHaLW?w=201&h=309&c=7&r=0&o=5&dpr=1.3&pid=1.7" },
            { title : "Sandman", author : "Neil Gaiman and various artists", price : 19.99, image : "https://th.bing.com/th/id/OIP.SW5qSOMxa2OMfeS-YgSQxwHaLB?w=202&h=301&c=7&r=0&o=5&dpr=1.3&pid=1.7   " },
            { title: "The Walking Dead" , author: "Robert Kirkman and various artists" , price: 14.99, image: "data:image/jpeg;base64," },
            { title: "Black Hammer" , author: "Jeff Lemire and Dean Ormston" , price: 14.99  , image: "https://th.bing.com/th/id/OIP.3Nl0zeb41LOaVUjXLxtFYAHaLZ?w=201&h=310&c=7&r=0&o=5&dpr=1.3&pid=1.7" },
            { title: "The Vision" , author: "Tom King and Gabriel Hernandez Walta" , price: 17.99 , image: "https://th.bing.com/th/id/OIP.7S0AKApveP88XJb0MV731wAAAA?w=123&h=207&c=7&r=0&o=5&dpr=1.3&pid=1.7" },
            { title: "Fun Home" , author: "Alison Bechdel" , price: 15.99 , image: "https://th.bing.com/th/id/OIP.NZTzUURVvS6MI-RPWcPA8gHaLG?w=202&h=303&c=7&r=0&o=5&dpr=1.3&pid=1.7" },
            { title: "Bone" , author: "Jeff Smith" , price: 44.95 , image: "https://th.bing.com/th/id/OIP.wMXX_ry92BlNOB9js0RtowHaLH?w=202&h=303&c=7&r=0&o=5&dpr=1.3&pid=1.7" }





        ];

        // Initialize the cart
        let cartItems = [];

        // Function to render the books
        function renderBooks() {
            const booksContainer = document.getElementById("books");
            booksContainer.innerHTML = "";

            books.forEach((book, index) => {
                const bookElement = document.createElement("div");
                bookElement.classList.add("book");
                bookElement.innerHTML = `
          <img src="${book.image}" alt="${book.title}">
          <span>${book.title} - ${book.author} ($${book.price.toFixed(2)})</span>
          <button onclick="addToCart(${index})">Add to Cart</button>
        `;
                booksContainer.appendChild(bookElement);
            });
           
        }
       
        // Function to render the shopping cart
        var totalCost = 0
        function renderCart() {
            const cartContainer = document.getElementById("cart");
            var cartContent = cartContainer.querySelectorAll(".cart-item");

            cartContent.forEach(function (itemDiv) {
                cartContainer.removeChild(itemDiv);
            });
            // cartContainer.innerHTML = "";
           
            cartItems.forEach((item, index) => {
                const cartItemElement = document.createElement("div");
                cartItemElement.classList.add("cart-item");
                cartItemElement.innerHTML = `
                <span>${item.title} - ${item.author} ($${item.price.toFixed(2)})</span>
                <button onclick="removeFromCart(${index})">Remove</button>
        `;
                cartContainer.appendChild(cartItemElement);
            });
           
            var totalCostElement = document.getElementById("totalCost");
            totalCostElement.textContent = totalCost.toFixed(2);
        }
       
       
       
        // Function to add a book to the cart
        renderBooks();
        function addToCart(bookIndex) {
            console.log(bookIndex)
           
            console.log(books[bookIndex])
            const selectedBook = books[bookIndex];
            cartItems.push(selectedBook);
            console.log(cartItems)
            // console.log('selected book' + selectedBook.price)
            totalCost += selectedBook.price
            renderCart();
        }
       
        // Function to remove a book from the cart
        function removeFromCart(cartIndex) {
            var selectedbook = cartItems[cartIndex]
            var requiredp = selectedbook.price
            totalCost -= requiredp.toFixed(2)
            cartItems.splice(cartIndex, 1);
            renderCart();
        }
       
        // Initial rendering
        renderBooks();
        renderCart();
        </script>

<style>
    body {
        font-family: Arial, sans-serif;
        text-align: center;
        background-color: #f0f0f0;
    }

    h1 {
            margin: 20px 0;
        }
       
        #bookstore {
            display: flex;
            flex-direction: column;
            max-width: 800px;
            margin: 0 auto;
            padding: 20px;
            background-color: #ffffff;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
        }

        .navbar {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 10px;
            background-color: #007bff;
            color: #fff;
        }

        .menu-button,
        .search-button {
            cursor: pointer;
            background-color: #007bff;
            color: #fff;
            border: none;
            padding: 8px 15px;
            border-radius: 3px;
            font-size: 14px;
            transition: background-color 0.3s ease;
        }

        .search-container {
            display: flex;
            align-items: center;
        }

        .search-container input[type="text"] {
            padding: 8px;
            border: 1px solid #ddd;
            border-radius: 3px;
            margin-right: 10px;
        }

        .menu-button:hover,
        .search-button:hover {
            background-color: #0056b3;
        }

        .grid-container {
            display: grid;
            grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));
            gap: 20px;
        }

        .book {
            display: flex;
            flex-direction: column;
            align-items: center;
            padding: 10px;
            border: 1px solid #315d9e;
            border-radius: 5px;
            background-color: #3d77c2;
            box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);
            justify-content: flex-end;
        }

        .book img {
            max-width: 150px;
            height: auto;
            margin-bottom: 10px;
            border-radius: 5px;
        }

        .book button {
            cursor: pointer;
            background-color: #91c1f5;
            color: #fafafa;
            border: 2px;
            border-color: #662323;
            padding: 8px 15px;
            border-radius: 3px;
            font-size: 14px;
            transition: background-color 0.3s ease;
        }

        .book button:hover {
            background-color: #0056b3;
        }

        #cart {
            display: flex;
            flex-direction: column;
        }

        .cart-item {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 10px;
            border: 1px solid #ddd;
            border-radius: 5px;
            background-color: #f9f9f9;
            box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);
        }

        .cart-item button {
            cursor: pointer;
            background-color: #dc3545;
            color: #fff;
            border: none;
            padding: 8px 15px;
            border-radius: 3px;
            font-size: 14px;
            transition: background-color 0.3s ease;
        }

        .cart-item button:hover {
            background-color: #c82333;
        }

        .total-cost {
            margin-top: 20px;
            padding: 10px;
            background-color: white;
            color: black;
            border: 2px solid black ;
            border-radius: 5px;
            box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);
        }

        .total-cost p {
            margin: 0;
        }

        #totalCost {
            font-size: 24px;
            font-weight: bold;
        }

        .order-button {
            cursor: pointer;
            background-color: #28a745;
            color: #fff;
            border: none;
            padding: 8px 15px;
            border-radius: 3px;
            font-size: 16px;
            transition: background-color 0.3s ease;
        }

        .order-button:hover {
            background-color: #218838;
        }
    </style>
</body>

</html>
